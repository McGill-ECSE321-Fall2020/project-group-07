		{
			"name": "createShipment1",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "cb256627-f979-4fd1-b93c-65501df598fa",
						"exec": [
							"pm.test(\"Successful POST request\", function () {\r",
							"    pm.expect(pm.response.code).to.be.oneOf([201, 202]);\r",
							"});\r",
							"\r",
							"pm.test(\"non-null dto returned\", function(){\r",
							"    let dto=pm.response.json();\r",
							"    pm.expect(dto).is.not.undefined;\r",
							"});\r",
							"\r",
							"pm.test(\"source address match\",function(){\r",
							"    let dto=pm.response.json();\r",
							"    pm.expect(dto.sourceAddress).to.eql(\"3415 girouard, montreal, QC\");\r",
							"})\r",
							"\r",
							"pm.test(\"destination address match\",function(){\r",
							"    let dto=pm.response.json();\r",
							"    pm.expect(dto.destinationAddress).to.eql(\"1000 sherbrooke, montreal, QC\");\r",
							"})\r",
							"\r",
							"pm.test(\"shipping cost match\",function(){\r",
							"    let dto=pm.response.json();\r",
							"    pm.expect(dto.shippingCost).to.eql(25.6);\r",
							"})\r",
							"\r",
							"pm.test(\"total cost match\",function(){\r",
							"    pm.expect(pm.response.json().totalCost).to.eql(4605.41);\r",
							"})\r",
							"\r",
							"pm.test(\"shipment status match\",function(){\r",
							"    pm.expect(pm.response.json().shipmentStatus).to.eql(\"CREATED\");\r",
							"})\r",
							"\r",
							"pm.test(\"recipient name match\",function(){\r",
							"    pm.expect(pm.response.json().recipientName).to.eql(\"natalia\");\r",
							"})\r",
							"pm.test(\"customer id match\",function(){\r",
							"    pm.expect(pm.response.json().customerId).to.eql(pm.globals.get(\"NatcustomerId1\"));\r",
							"\r",
							"})\r",
							"pm.test(\"purchases ids match\",function(){\r",
							"    pm.expect(pm.response.json().purchases).to.contains(pm.globals.get(\"NatpurchaseId1\"));\r",
							"\r",
							"})\r",
							"pm.test(\"purchases ids match\",function(){\r",
							"    pm.expect(pm.response.json().purchases).to.contains(pm.globals.get(\"NatpurchaseId2\"));\r",
							"\r",
							"})\r",
							"\r",
							"pm.globals.set(\"NatshipmentId1\",pm.response.json().shipmentId);"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\r\n    \"shipmentId\" : 5005,\r\n    \"sourceAddress\" : \"3415 girouard, montreal, QC\", \r\n    \"destinationAddress\" : \"1000 sherbrooke, montreal, QC\",\r\n    \"recipientName\" : \"natalia\",\r\n    \"shippingCost\" : 25.6, \r\n    \"totalCost\" : 70.72,\r\n    \"customerId\" :{{NatcustomerId1}},\r\n    \"shipmentStatus\" : 0, \r\n    \"purchases\" : [{{NatpurchaseId1}},{{NatpurchaseId2}}]\r\n}",
